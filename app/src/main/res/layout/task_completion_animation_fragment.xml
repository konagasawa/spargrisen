            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:src="@drawable/spagrisen_purple"
            >
        </ImageView>
        <ImageView
            android:id="@+id/skyblueGris"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:src="@drawable/spagrisen_skyblue"
            >
        </ImageView>
        <ImageView
            android:id="@+id/blueGrisSecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:src="@drawable/spagrisen_blue"
            >
        </ImageView>
        <ImageView
            android:id="@+id/greenGrisSecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:src="@drawable/spagrisen_green"
            >
        </ImageView>
        <ImageView
            android:id="@+id/limeGrisSecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:src="@drawable/spagrisen_lime"
            >
        </ImageView>
        <ImageView
            android:id="@+id/yellowGrisSecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:src="@drawable/spagrisen_yellow"
            >
        </ImageView>
        <TextView
            android:id="@+id/taskCompleteMessageTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:textSize="@dimen/taskCompleteMessageTextSize"
            android:text="@string/taskCompleteMessage"
            >
        </TextView>


